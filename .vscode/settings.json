{
    // Enable Material Icon Theme for these settings to take effect
    "workbench.iconTheme": "material-icon-theme",

    // Custom icon themes
    "material-icon-theme.files.customClones": [
        {
            "name": "rust-mod",
            "base": "rust",
            "color": "blue-400",
            "fileNames": ["mod.rs"]
        },
    ],

    // Custom icons for specific folders
    "material-icon-theme.folders.associations": {
        // CLI Tool
        "cli-client": "client",
        "cli-client/src": "client",

        // Server
        "server": "server",
        "server/dist": "server",
        "server/src": "server",

        // Shared code
        "shared": "shared",
        "shared/src": "shared",

        // Generated by Cargo
        "target": "target",
        "target-build": "target",
    },

    // Custom icons for specific file types
    "material-icon-theme.files.associations": {

    },


}